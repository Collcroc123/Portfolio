version 35
__lua__
loops = 1000
ascend = true
string = "♥"
music(1)

for i = 0, loops do
	if (i % 10 == 0) then
		ascend = not ascend
	else
		if (ascend) then
			string = string.."♥"
			print(string,14)
		else
			string = sub(string,1,-2)
			print(string,81)
		end
	end
end

