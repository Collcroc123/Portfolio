version 35
__lua__
pos={x=0,y=0}
p={col=10,row=10}

function _init()
 cls()
 palt(11, true) -- beige color as transparency is true
 palt(0, false) -- black color as transparency is false
end

function _draw()
	cls(1)
	camera(3,3)
	map()
	spr(16,pos.x+48,pos.y+48)
	if(btnp(üÖæÔ∏è)) then
		spr(36,pos.x+48,pos.y+48)
		sfx(2)
	end
	if(btnp(‚ùé)) then
		spr(40,pos.x+48,pos.y+48)
		sfx(1)
	end
end

function _update()
	if(btnp(‚¨ÖÔ∏è)) then
		if(pos.x > 0) pos.x-=8 sfx(0)
	end
	if(btnp(‚û°Ô∏è)) then
		if(pos.x<72) pos.x+=8 sfx(0)
	end
	if(btnp(‚¨ÜÔ∏è)) then
		if(pos.y>0) pos.y-=8 sfx(0)
	end
	if(btnp(‚¨áÔ∏è)) then
		if(pos.y<72) pos.y+=8 sfx(0)
	end
end

