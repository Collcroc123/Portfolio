function get_step(t)

 local li=1

 t%=sl*m

 while li<#l and l[li][1]*m<=t do
  li+=1
 end
 li=(li-1)%#l

 local mtype=false
 if #l[li]>=3 and l[li][3] then
  mtype=true
 end

 return {
  l[li],
  l[li][1]*m==t,
  flr(t%(m*2))==0,
  mtype
 }

end

