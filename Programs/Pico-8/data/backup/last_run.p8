pico-8 cartridge // http://www.pico-8.com
version 35
__lua__
pos={x=0,y=0}
p={col=10,row=10}

function _init()
 cls()
 palt(11, true) -- beige color as transparency is true
 palt(0, false) -- black color as transparency is false
end

function _draw()
	cls(1)
	camera(3,3)
	map()
	spr(44,
		pos.x+48,pos.y+48)
end

function _update()
	if (btnp(⬅️)) then
		if (pos.x > 0) pos.x-=8
	end
	if (btnp(➡️)) then
		if (pos.x < 72) pos.x+=8
	end
	if (btnp(⬆️)) then
		if (pos.y > 0) pos.y-=8
	end
	if (btnp(⬇️)) then
		if (pos.y < 72) pos.y+=8
	end
end
