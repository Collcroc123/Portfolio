function _update60()

 tm=t%(sl*m)

  if btnp(3) then
   debug=not debug
  end


  -- start music again in sync with t. the sync between graphics and audio in pico8 drifts on my machine
  if(flr(tm)==0)music(0)


  if btnp(0) then
    ptn = flr((tm-15)/(m*16))%11
    music(ptn)
    t=ptn*(m*16)
  end

  if btnp(1) then
    ptn = flr((tm+(m*16))/(m*16))%11
    music(ptn)
    t=ptn*(m*16)
  end


 if debug then

  --magically slow music down by half
  poke(0x5f40,15) 

  tinc=.5

 else

  --magically slow music down by half
  poke(0x5f40,0) 

  tinc=1

 end

--30fps
--tinc=tinc*2

 t+=tinc
 mt-=tinc

end

function _draw()

